<Page x:Class="ShopClient.RegisterPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ShopClient"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="498.374"
      Title="RegisterPage">

    <Grid Margin="0,0,-125,0">
        <Frame x:Name="_NavigationFrame" NavigationUIVisibility="Hidden" Margin="0,0,156,0" />
        <TextBox x:Name="username" HorizontalAlignment="Left" Height="23" Margin="211,58,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="155"/>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="143,58,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="63" RenderTransformOrigin="0.548,0.989" Height="23"><Run Text="Username"/><Run Text=":"/></TextBlock>
        <Button x:Name="register" Content="Register" HorizontalAlignment="Left" Margin="291,108,0,0" VerticalAlignment="Top" Width="75" Click="Register_Click"/>
        <Button x:Name="login" Content="Login" HorizontalAlignment="Left" Margin="211,108,0,0" VerticalAlignment="Top" Width="75" Click="Back_Click"/>

    </Grid>
</Page>
