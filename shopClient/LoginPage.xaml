<Page x:Class="ShopClient.LoginPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:ShopClient"
      mc:Ignorable="d" 
      d:DesignHeight="300"
      Title="LoginPage" Width="560.976">

    <Grid >
        <Frame x:Name="_NavigationFrame" NavigationUIVisibility="Hidden" />
        <TextBox x:Name="username" HorizontalAlignment="Left" Height="23" TextWrapping="Wrap" Text="username" VerticalAlignment="Top" Width="198" Margin="176,68,0,0"/>
        <Button x:Name="button"  Click="Login_Click" Content="Login" HorizontalAlignment="Left" Margin="176,134,0,0" VerticalAlignment="Top" Width="75"/>
        <TextBox x:Name="password" HorizontalAlignment="Left" Height="23" Margin="176,96,0,0" TextWrapping="Wrap" Text="password" VerticalAlignment="Top" Width="198"/>
        <Button x:Name="register" Click="Register_Click"  Content="Register" HorizontalAlignment="Left" Height="20" Margin="296,134,0,0" VerticalAlignment="Top" Width="78"/>
    </Grid>
</Page>
